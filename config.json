{
  "DB": {
    "USER": "root",
    "PASSWORD": "...",
    "ADDRESS": "localhost",
    "PORT": "3306",
    "NAME": "krtkobrani",
    "DRIVER": "mysql+pymysql"
  },

  "LOGGING": {
    "version": 1,
    "formatters": {"default": {
        "format": "[%(asctime)s] %(name)s %(levelname)s in %(module)s: %(message)s"
    }},
    "handlers": {"file": {
        "class": "logging.handlers.RotatingFileHandler",
        "filename": "krtkobrani.log",
        "formatter": "default",
        "maxBytes": 5242880,
        "backupCount": 3
    }},
    "root": {
        "level": "DEBUG",
        "handlers": [
          "file"
        ]
    }
  }
}